  <ItemGroup>
    <ProjectReference Include="..\CoreCT\CoreCT.csproj" />
    <ProjectReference Include="..\DTCore5.0-exp\DTMath\DTMath.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="MessageResources\LocalizedMessages.Designer.cs">
      <DesignTime>True</DesignTime>
